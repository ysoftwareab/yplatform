*.mdown        text
*.mkd          text
*.mkdn         text

# NOTE skipping *.m, because it matches Matlab/ObjectiveC too
# *.m            text
*.mu           text

# NOTE skipping *.m, because it matches Mathematica/Matlab too
# *.m            text
*.mm           text

*.p            text
